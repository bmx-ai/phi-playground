    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run GSM8K test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/models/pytorch/eval_gsm8k.py",
            "console": "integratedTerminal",
            "args": [
                "--model_module", "pytorch_microsoft_phi_model",
                "--model_storage", "${workspaceFolder}/.cache/models/microsoft/phi-2",
                "--evaluation_module", "gsm8k",
                "--evaluation_storage",  "${workspaceFolder}/.cache/datasets/gsm8k" 
            ],
            "env":{
                "PYTHONPATH": "${workspaceFolder}/models/pytorch"
            }
        }
    ]
}